# configurar porta do Tomcat
server.port = 8088

# configurar usuario do banco
spring.datasource.username = root

# configurar a sinha de acesso ao banco
spring.datasource.password = mysql

# configurar URL do banco
spring.datasource.url = jdbc:mysql://lolocalhost:3306/dashcard?useTimezone=true&serverTimezone=UTC

